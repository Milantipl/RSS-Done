@model ConnectionLibrary.Model.Result
  <input type="hidden" id="hdntotal" name="Total" value="@Model.Total" />
<table class="table table-bordered">
                        <thead class="m-head">
                            <tr>
                                <th>માસ</th>
                                <th>નગર</th>
                                <th>ભાગ</th>
                                <th colspan="3">Action</th>
                            </tr>
                            <tr>
                                <th colspan="3"></th>
                                <th style="width: 8%;">શાખા</th>
                                <th style="width: 8%;">મિલન</th>
                                <th style="width: 8%;">બેઠક</th>
                                <th style="width: 8%;">નગર</th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model.ViewNagarVrut != null && Model.ViewNagarVrut.Count > 0)
                            {
                                foreach (var item in Model.ViewNagarVrut)
                                {
                                <tr>
                                    <td>
                                        @if (@item.Month.ToString() != "0")
                                        {
                                            @item.Month
                                        }
                                        else
                                        {
                                            <input type="text" class="form-control" id="txtMonth" readonly="" style="cursor: not-allowed;" />
                                        }

                                    </td>
                                    <td>@item.Nagar</td>
                                    <td>@item.Bhag</td>
                                    <td>
                                       @if (@item.ShakhaCount.ToString() != "0")
                                        {
                                            @* <a title="Edit"  class="btn btn-info btn-sm" data-toggle="modal"   OnClick="javascript: UpdateShakha();"
                                                data-target="#addShakha">
                                                @item.ShakhaCount</a>*@
                                            <a OnClick="javascript: UpdateShakha(@item.NagarID,@item.N_MonthID,'@item.Bhag');" title="Edit" class="btn btn-info btn-sm" data-toggle="modal" data-target="#addShakha">  @item.ShakhaCount</a>
                                        }
                                        else
                                        {
                                            @item.ShakhaCount
                                        }
                                    </td>
                                    <td>
                                        @if (@item.MilanCount.ToString() != "0")
                                        {
                                           <a OnClick="javascript: UpdateMilan(@item.NagarID,@item.N_MonthID,'@item.Bhag');" title="Edit" class="btn btn-info btn-sm" data-toggle="modal" data-target="#addMilan">  @item.MilanCount</a>
                                        }
                                        else
                                        {
                                            @item.MilanCount
                                        }
                                    </td>
                                     <td>
                                        @if (@item.BethakCount.ToString() != "0")
                                        {
                                           <a OnClick="javascript: UpdateBethak(@item.NagarID,@item.N_MonthID,'@item.Bhag');" title="Edit" class="btn btn-info btn-sm" data-toggle="modal" data-target="#addBethak">  @item.BethakCount</a>
                                        }
                                        else if(@item.BCount.ToString()!="0")
                                        {
                                           <a OnClick="javascript: UpdateBethak(@item.NagarID,@item.N_MonthID,'@item.Bhag');" title="Edit" class="btn btn-info btn-sm" data-toggle="modal" data-target="#addBethak">  @item.BethakCount</a>
                                        }
                                         else
                                         {
                                            @item.BethakCount
                                        }
                                    </td>
                                    
                                    <td>
                                        @if (@item.NVID.ToString() != "0")
                                        {
                                            <a OnClick="javascript: UpdateNagar(@item.NVID,@item.NagarID,@item.N_MonthID,@item.Bethak,'@item.Bvarg.Trim()',@item.BvargUP,'@item.AbyasVarg.Trim()',@item.Apekshit,@item.Upsthit,'@item.Ektrikaran.Trim()',@item.N_Tarun,@item.N_Bal,@item.N_Yog,@item.N_Shishu,'@item.Bhag');" data-toggle="modal" data-target="#addNagar">
                                                <img src="/Content/dist/img/correct.png" style="width: 30px;" /></a>
                                        }
                                        else
                                        {
                                            <img src="/Content/dist/img/cross.png" style="width: 30px;" />
                                        }
                                    </td>
                                </tr>
                                }
                            }
                            else
                            {
                                 <tr>
                        <td colspan="5">
                            <h4>No Record Found !!</h4>
                        </td>

                    </tr>
                            }

                        </tbody>
                    </table>