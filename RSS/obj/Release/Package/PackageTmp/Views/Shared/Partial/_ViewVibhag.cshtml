@model ConnectionLibrary.Model.Result
<div class="card card-info">
                <div class="card-header">
                    <h3 class="card-title">વિભાગ</h3>
                    <div class="card-tools">
                        <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fas fa-minus"></i></button>
                    </div>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                    <table class="table table-bordered">
                        <thead class="m-head">
                            <tr>
                                <th>વિભાગ </th>
                                <th>કુલ ભાગ </th>
                                <th>કુલ નગર </th>
                                <th>કુલ વસ્તી </th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model.ViewVibhag != null)
                            {
                                foreach (var item in Model.ViewVibhag)
                                {
                                <tr>
                                    <td>@item.Vibhag</td>
                                    <td>@item.BhagCount</td>
                                    <td>@item.NagarCount</td>
                                    <td>@item.VastiCount</td>

                                </tr>
                                            
                                }
                            }
                        </tbody>
                    </table>
                </div>
            </div>
