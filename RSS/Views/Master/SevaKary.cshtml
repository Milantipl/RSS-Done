@model ConnectionLibrary.Model.Result
@{
    ViewBag.Title = "SevaKary";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">સેવાકાર્ય</h1>
                </div>
                <!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="@Url.Action("Dashboard","Master")">Home</a></li>
                        <li class="breadcrumb-item active">સેવાકાર્ય</li>
                    </ol>
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>
    <section class="content">
        <div class="container-fluid">
            <!-- SELECT2 EXAMPLE -->
            <div class="row">
                <div class="offset-md-3 col-md-6">
                    <div class="card card-info">
                        <div class="card-header">
                            <h3 class="card-title">સેવાકાર્ય</h3>
                            <div class="card-tools">
                                <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fas fa-minus"></i></button>
                            </div>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <table class="table table-bordered">
                                <thead class="m-head">
                                    <tr>
                                        <th>સેવાકાર્ય</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model.ListSevaKary != null)
                                    {
                                        foreach (var item in Model.ListSevaKary)
                                        {
                                        <tr>
                                            <td>@item.SevaKary</td>

                                        </tr>
                                            
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.card -->
        </div>
        <!-- /.container-fluid -->
    </section>
</div>
<!-- /.content-wrapper -->
@section scripts
{
    <script type="text/javascript">
        $(document).ready(function () {
            $("a").filter(".nav-link active").removeClass("nav-link active");
            $("#SevaKary").addClass("nav-link active");
            $("#Setting").addClass("nav-link active");
            $("li").filter(".nav-item menu-open").removeClass("nav-item menu-open");
            $("#Settingli").addClass("nav-item menu-open");
        });
    </script>
}