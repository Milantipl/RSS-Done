@model ConnectionLibrary.Model.Result
<div class="card card-info">
                        <div class="card-header">

                             @if (Model.ViewPravasikaryakarta != null)
        {
            <h3 class="card-title">પ્રવાસી કાર્યકર્તા (Total: @Model.ViewPravasikaryakarta.Count)</h3>
        }
        else
        {
            <h3 class="card-title">પ્રવાસી કાર્યકર્તા</h3>
        }

                         
                            <div class="card-tools">
                                <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fas fa-minus"></i></button>
                            </div>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-12 text-right">
                                    <button type="button" class="btn btn-primary mb-1" data-toggle="modal" data-target="#myModal">Add New</button>
                                </div>
                                
                            </div>
                            <table class="table table-bordered">
                                <thead class="m-head">
                                    <tr>
                                        <th>નામ</th>
                                        <th>મોબાઈલ નમ્બર</th>
                                        <th>દાયિત્વ </th>
                                        <th>સ્તર </th>
                                        <th>વસ્તી</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                     @if (Model.ViewPravasikaryakarta != null)
                                     {
                                         foreach (var item in Model.ViewPravasikaryakarta)
                                         {
                                    <tr>
                                        <td>@item.Name </td>
                                        <td>@item.MobileNo</td>
                                        <td>@item.Dayitva</td>
                                        <td>@item.Star</td>
                                         <td>@item.vasti</td>
                                        <td>
                                            <a OnClick="javascript: Update(@item.Pravasi_karyakartaID,'@item.Name','@item.MobileNo',@item.VastiID,@item.DayitvaID,@item.StarID);" title="Edit" class="btn btn-info btn-sm" data-toggle="modal" data-target="#myModal"><i class="fa fa-edit"></i></a>
                                            <a title="Delete" href="javascript:Delete(@item.Pravasi_karyakartaID,@item.NagarID);" OnClick="javascript:return confirm('Are you sure you want to delete this Record ?')" class="btn btn-danger btn-sm"><i class="fa fa-times"></i></a>

                                        </td>
                                    </tr>
                                         }
                                     }
                                    
                                </tbody>
                            </table>
                        </div>


                        
                    </div>