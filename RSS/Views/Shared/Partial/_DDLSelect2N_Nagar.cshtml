@model ConnectionLibrary.Model.Result

<select class="form-control" id="drpN_Nagar" name="NagarID" onchange="BindNagardata(this);">
    @if (Model.ListNagar != null && Model.ListNagar.Count > 0)
    {
        <option value="@Model.ListNagar.FirstOrDefault().NagarID" selected="selected">@Model.ListNagar.FirstOrDefault().Nagar</option>
    }
</select>






