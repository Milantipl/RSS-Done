@model ConnectionLibrary.Model.Result

<div class="card card-info">

    <div class="card-header">
        <h3 class="card-title">ભાગ</h3>
        <div class="card-tools">
            <button type="button" class="btn btn-tool" data-card-widget="collapse"><i class="fas fa-minus"></i></button>
        </div>
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <div class="row">
            <div class="col-md-12 text-right">
                <a class="btn btn-primary mb-1" data-toggle="modal" data-target="#myModal" onclick="javascript:Clear();">Add New</a>
            </div>

        </div>
        <table class="table table-bordered">
            <thead class="m-head">
                <tr>
                    <th>Name </th>

                    <th>Document </th>
                    <th>Description </th>

                </tr>
            </thead>
            <tbody>
                @if (Model.ListDoc != null)
                {
                    foreach (var item in Model.ListDoc)
                    {
                        <tr>


                            <td>@item.name</td>
                            <td>@item.doc</td>
                            <td>@item.description</td>
                        </tr>

                    }
                }
            </tbody>
        </table>
    </div>
</div>
